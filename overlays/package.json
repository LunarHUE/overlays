{
  "name": "@lunarhue/overlays",
  "version": "1.0.3",
  "description": "Type-safe overlay management for React and Next.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "zod": "^4.2.1"
  },
  "author": "LunarHUE",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LunarHUE/overlays"
  },
  "publishConfig": {
    "access": "public"
  }
}